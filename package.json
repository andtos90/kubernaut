{
  "name": "kubernaut",
  "version": "1.0.0",
  "description": "Manages kubernetes deployments",
  "main": "server/build/index.js",
  "scripts": {
    "start": "concurrently 'npm run start-server' 'npm run start-client'",
    "start-server": "npm run start-server-dependencies && nodemon server/index.js --exec babel-node --ignore client/",
    "start-client": "true",
    "start-server-dependencies": "docker-compose up -d local-dependencies",
    "test": "npm run test-server && npm run test-client",
    "test-server": "APP_ENV=test jest",
    "test-server-w": "APP_ENV=test jest --watch",
    "test-client": "true",
    "test-client-w": "true",
    "build": "concurrently 'npm run build-server' 'npm run build-client'",
    "build-server": "rm -rf server/build && babel --out-dir server/build ./server --source-maps --copy-files",
    "build-client": "true",
    "prepush": "IMAGE=kubernaut TAG=local docker-compose run --entrypoint npm build-kubernaut run test-server -- --ci --bail --no-colors --verbose",
    "lint": "eslint ."
  },
  "author": "TES",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chance": "^1.0.11",
    "concurrently": "^3.5.0",
    "eslint": "^4.4.1",
    "eslint-config-imperative-es6": "^2.0.0",
    "eslint-plugin-react": "^7.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0-beta.1",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "boom": "^5.2.0",
    "bunyan": "^1.8.12",
    "chalk": "^2.1.0",
    "confabulous": "^1.5.3",
    "groundhog-day": "^1.1.1",
    "hogan.js": "^3.0.2",
    "js-yaml": "^3.10.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lru-cache": "^4.1.1",
    "marv": "^1.5.2",
    "marv-pg-driver": "^1.4.0",
    "multer": "^1.3.0",
    "on-headers": "^1.0.1",
    "prepper": "^1.1.0",
    "ramda": "^0.24.1",
    "safe-json-parse": "^4.0.0",
    "systemic": "^1.3.2",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.1.0",
    "systemic-pg": "^1.2.0",
    "uuid": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tes/kube-hackday.git"
  },
  "bugs": {
    "url": "https://github.com/tes/kube-hackday/issues"
  },
  "homepage": "https://github.com/tes/kube-hackday#readme"
}
